{
  "name": "@anorcle/dsa",
  "version": "0.3.2",
  "description": "Implementation of Complex Data Structures and Algorithms for Web",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "docs": "typedoc --options typedoc.json",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "publish": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anorcle/dsa.git.git"
  },
  "keywords": [
    "data-structures",
    "algorithms",
    "dsa",
    "stl"
  ],
  "author": "Daniel Zuzevich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anorcle/dsa.git/issues"
  },
  "homepage": "https://anorcle.github.io/dsa/",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.11.0",
    "eslint-plugin-jest": "^24.1.0",
    "jest": "^26.6.0",
    "prettier": "^2.3.0",
    "ts-jest": "^26.4.1",
    "typedoc": "^0.22.15",
    "typescript": "^4.0.3"
  },
  "files": [
    "dist/**/*"
  ]
